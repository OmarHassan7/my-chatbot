{
    "version": 2,
    "builds": [
        {
            "src": "api/chat/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/test/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/test",
            "dest": "/api/test/index.py",
            "methods": [
                "GET",
                "POST",
                "OPTIONS"
            ]
        },
        {
            "src": "/api/chat",
            "dest": "/api/chat/index.py",
            "methods": [
                "GET",
                "POST",
                "OPTIONS"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}